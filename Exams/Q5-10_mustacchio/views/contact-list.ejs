<h1><span>contact requests</span></h1>
		<table>
			
			<thead>
			<tr>
				<th>Date</th>
				<th>Name</th>
				<th>Message</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% for (let i = 0; i < items.length; i++) { %>
				<tr>
					<td><%=items[i].datePosted%></td>
					<td><%=items[i].name%></td>
					<td><%=items[i].message%></td>
					<td><a href="/contacts/<%=items[i].id%>/edit" class="button1">View</a></td>
				</tr>
			<% } %> 
		</tbody>
		</table>